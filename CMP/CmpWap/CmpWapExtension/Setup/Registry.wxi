                     Name="ProductCode"
                     Value="[ProductCode]"
                     KeyPath="yes" />
      <RegistryValue Type="string"
                     Name="ProductVersion"
                     Value="[ProductVersion]" />
      <RegistryValue Type="string"
                     Name="InstallPath"
                     Value="[INSTALLDIR]" />
      <RegistryValue Type="string"
                     Name="WebSitePath"
                     Value="[WEBSITEDIR]" />
      <RegistryValue Type="string"
                     Name="AdminSitePath"
                     Value="[PATHADMINSITE]" />
      <RegistryValue Type="string"
                     Name="TenantSitePath"
                     Value="[PATHTENANTSITE]" />
      <RegistryValue Id="InstanceNameRegKey"
                  Name="InstanceName" Type="string" Value="[WAPINSTANCENAME]"  />
      <RegistryValue Id="DatabaseNameRegKey"
                        Name="DatabaseName" Type="string" Value="[WAPDATABASENAME]" />
      <RegistryValue Id="ConnectionStringRegKey"
