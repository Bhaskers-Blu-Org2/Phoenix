<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    x:Class="CMP.Setup.SplashPage"
    x:Uid="Window_1"
    Height="310"
    ResizeMode="NoResize"
    Title="{DynamicResource wizardTitle}"
    Width="476"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None">
    <Window.Resources>
        <ResourceDictionary
            x:Uid="ResourceDictionary_1">
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary
                    x:Uid="ResourceDictionary_2"
                    Source="/SetupCMP;component/WPFResourceDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Image
        x:Name="ImageSplashPage"
        x:Uid="ImageSplashPage"
        Source="/SetupCMP;component/Images/SplashScreen.png"
        Stretch="None"></Image>
</Window>
